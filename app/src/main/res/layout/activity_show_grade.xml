<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/choiceeducationbg"
    tools:context=".show_grade">

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="83dp"
        android:layout_height="72dp"
        android:background="#00000000"
        android:onClick="onclick"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.905"
        app:layout_constraintStart_toEndOf="@+id/nurse_id"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.035"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="96dp"
        android:layout_height="102dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="1168dp"
        android:layout_marginRight="1168dp"
        android:background="#00000000"
        android:onClick="back"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@drawable/back" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="1068dp"
        android:layout_height="513dp"
        android:layout_marginLeft="70dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.646"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.721">

        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="1021dp"
            android:layout_height="62dp"
            android:orientation="horizontal"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.398">

            <TextView
                android:layout_width="115dp"
                android:layout_height="62dp"
                android:background="@drawable/choiceedubutton"
                android:gravity="center"
                android:text="作答"
                android:textSize="30dp" />

            <TextView
                android:layout_width="906dp"
                android:layout_height="62dp"
                android:layout_marginLeft="10dp"
                android:background="@drawable/choiceedubutton"
                android:gravity="center"
                android:text="題目"
                android:textSize="30dp" />

        </LinearLayout>

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="wrap_content"
            android:layout_height="372dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.883">

            <TableLayout
                android:layout_width="1021dp"
                android:layout_height="420dp"
                android:orientation="horizontal">

                <TableRow
                    android:layout_width="1021dp"
                    android:layout_height="160dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/txv_right_wrong_1"
                        android:layout_width="115dp"
                        android:layout_height="160dp" />

                    <TextView
                        android:id="@+id/txv_Q1"
                        android:layout_width="906dp"
                        android:layout_height="160dp"
                        android:layout_marginLeft="10dp"
                        android:text="4"
                        android:textSize="30sp" />
                </TableRow>

                <TableRow
                    android:layout_width="1021dp"
                    android:layout_height="160dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/txv_right_wrong_2"
                        android:layout_width="115dp"
                        android:layout_height="160dp" />

                    <TextView
                        android:id="@+id/txv_Q2"
                        android:layout_width="906dp"
                        android:layout_height="160dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginStart="10dp"
                        android:text="4"
                        android:textSize="30sp" />
                </TableRow>

                <TableRow
                    android:layout_width="1021dp"
                    android:layout_height="160dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/txv_right_wrong_3"
                        android:layout_width="115dp"
                        android:layout_height="160dp" />

                    <TextView
                        android:id="@+id/txv_Q3"
                        android:layout_width="906dp"
                        android:layout_height="160dp"
                        android:layout_marginLeft="10dp"
                        android:text="4"
                        android:textSize="30sp" />


                </TableRow>

                <TableRow
                    android:layout_width="1021dp"
                    android:layout_height="160dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/txv_right_wrong_4"
                        android:layout_width="115dp"
                        android:layout_height="160dp" />

                    <TextView
                        android:id="@+id/txv_Q4"
                        android:layout_width="906dp"
                        android:layout_height="160dp"
                        android:layout_marginLeft="10dp"
                        android:text="4"
                        android:textSize="30sp" />


                </TableRow>

                <TableRow
                    android:layout_width="1021dp"
                    android:layout_height="160dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/txv_right_wrong_5"
                        android:layout_width="115dp"
                        android:layout_height="160dp" />

                    <TextView
                        android:id="@+id/txv_Q5"
                        android:layout_width="906dp"
                        android:layout_height="160dp"
                        android:layout_marginLeft="10dp"
                        android:text="4"
                        android:textSize="30sp" />
                </TableRow>

            </TableLayout>
        </ScrollView>
    </LinearLayout>

    <TextView
        android:id="@+id/nurse_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="60dp"
        android:layout_marginStart="60dp"
        android:text="TextView"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.256"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.063" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="TextView"
        android:textSize="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <TextView
        android:id="@+id/tev_id"
        android:layout_width="225dp"
        android:layout_height="54dp"
        android:text="TextView"
        android:textSize="30sp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.984"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.159" />

    <TextView
        android:id="@+id/patient_id"
        android:layout_width="214dp"
        android:layout_height="57dp"
        android:gravity="center"
        android:text="TextView"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.156" />

</androidx.constraintlayout.widget.ConstraintLayout>